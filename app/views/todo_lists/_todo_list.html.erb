<%= turbo_frame_tag todo_list do %>
  <li class="list-group-item">
    <div class="d-flex justify-content-between" id="<%= dom_id todo_list %>">
      <span>
        <%= link_to todo_list.title, todo_list, data: { turbo_frame: "_top" } %>
      </span>
      <div>
        <%= link_to edit_todo_list_path(todo_list), class: "btn btn-success" do %>
          <i class="fa-solid fa-edit"></i>
        <% end %>
        <%= link_to todo_list, data: { turbo_method: :delete }, class: "btn btn-danger" do %>
          <i class="fa-solid fa-x"></i>
        <% end %>
      </div>
    </div>
  </li>
<% end %>